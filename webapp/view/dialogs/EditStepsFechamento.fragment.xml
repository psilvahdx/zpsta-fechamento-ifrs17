<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:f="sap.ui.layout.form">
    <Dialog title="{i18n>etapa_fechamento_tit_txt}" id="editFechamentoDialog" contentWidth="80%" draggable="true" resizable="true">
        <VBox class="sapUiSmallMargin">
            <f:SimpleForm
                id="headerFechamentoForm"
                editable="false"
                layout="ResponsiveGridLayout"
                labelSpanXL="4"
                labelSpanL="4"
                labelSpanM="12"
                labelSpanS="12"
                adjustLabelSpan="false"
                emptySpanXL="0"
                emptySpanL="0"
                emptySpanM="0"
                emptySpanS="0"
                columnsXL="2"
                columnsL="2"
                columnsM="1"
                singleContainerFullSize="false"
                title="{i18n>empresa_txt}: {fechamentoIFRS17Model>Bukrs}"
            >
                <f:content>
                    <Label text="{i18n>ano_txt}" />
                    <Text text="{fechamentoIFRS17Model>Ano}" />
                    <Label text="{i18n>mes_txt}" />
                    <Text text="{fechamentoIFRS17Model>Mes}" />
                </f:content>
            </f:SimpleForm>
            <f:SimpleForm
                id="prgFechamentoForm"
                editable="true"
                title="{i18n>prg_desc_txt}"
                layout="ResponsiveGridLayout"
                labelSpanXL="4"
                labelSpanL="4"
                labelSpanM="12"
                labelSpanS="12"
                adjustLabelSpan="false"
                emptySpanXL="0"
                emptySpanL="0"
                emptySpanM="0"
                emptySpanS="0"
                columnsXL="2"
                columnsL="2"
                columnsM="1"
                singleContainerFullSize="false"
            >
                <f:content>
                    <Label text="{i18n>status_txt}" />
                    <ComboBox id="cbStatusPrg" selectedKey="{fechamentoIFRS17Model>Prg/Statusarq}" enabled="{fechamentoIFRS17Model>Prg/enabled}" selectionChange=".onSelectionStatusChange">
                        <items>
                            <core:Item key="5" text="{i18n>para_validar_txt}" enabled="false"/>
                            <core:Item key="1" text="{i18n>valido_txt}" enabled="{path: 'fechamentoIFRS17Model>Prg/Statusarq', formatter: '.formatter.getListEnabledStatusByStatus'}" />
                            <core:Item key="2" text="{i18n>invalido_txt}"  enabled="false" />
                            <core:Item key="3" text="{i18n>invalidado_txt}" enabled="{path: 'fechamentoIFRS17Model>Prg/Statusarq', formatter: '.formatter.getListEnabledStatusByStatus'}" />
                            <core:Item key="999" text="{i18n>sem_arquivo_txt}"  enabled="false" />
                        </items>
                    </ComboBox>
                    <Label text="{i18n>processar_txt}" />
                    <Switch state="{fechamentoIFRS17Model>Prg/Acao}" customTextOn="{i18n>sim_txt}" customTextOff="{i18n>nao_txt}" enabled="{fechamentoIFRS17Model>Prg/enabled}"
                    change=".onActionFechamentoChange">
                        <layoutData>
                            <FlexItemData growFactor="1" />
                        </layoutData>
                    </Switch>
                </f:content>
            </f:SimpleForm>

