<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:f="sap.ui.layout.form">
    <Dialog title="{i18n>etapa_fechamento_tit_txt}" id="displayFechamentoDialog" contentWidth="80%" draggable="true" resizable="true" >
        <VBox class="sapUiSmallMargin">
            <f:SimpleForm
                id="displayHeaderFechamentoForm"
                editable="false"
                layout="ResponsiveGridLayout"
                labelSpanXL="4"
                labelSpanL="4"
                labelSpanM="12"
                labelSpanS="12"
                adjustLabelSpan="false"
                emptySpanXL="0"
                emptySpanL="0"
                emptySpanM="0"
                emptySpanS="0"
                columnsXL="2"
                columnsL="2"
                columnsM="1"
                singleContainerFullSize="false"
                title="{i18n>empresa_txt}: {fechamentoIFRS17Model>Bukrs}"
            >
                <f:content>                
                    <Label text="{i18n>ano_txt}" />
                    <Text text="{fechamentoIFRS17Model>Ano}" />
                    <Label text="{i18n>mes_txt}" />
                    <Text text="{fechamentoIFRS17Model>Mes}" />
                </f:content>
            </f:SimpleForm>
            <f:SimpleForm
                id="displayPrgFechamentoForm"
