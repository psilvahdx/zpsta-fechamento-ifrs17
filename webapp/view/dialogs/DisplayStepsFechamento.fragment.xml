                emptySpanM="0"
                emptySpanS="0"
                columnsXL="2"
                columnsL="2"
                columnsM="1"
                singleContainerFullSize="false"
            >
                <f:content>
                    <Label text="{i18n>status_txt}" />                       
                     <ObjectStatus 
                                icon="{path: 'fechamentoIFRS17Model>Prg/Statusarq', formatter:'.formatter.formatStatusIconFechamento'}"
                                text="{path: 'fechamentoIFRS17Model>Prg/Statusarq', formatter:'.formatter.formatStatusTextFechamento'}" 
                                state="{path: 'fechamentoIFRS17Model>Prg/Statusarq', formatter:'.formatter.formatStatusFechamento'}" />
                     <Button id="btnPrgShowLogError" type="Transparent" text="{i18n>log_erros_txt}" press=".onShowLogErros"
                     visible="{= ${fechamentoIFRS17Model>Prg/Statusarq} === '2' ? true : false}"></Button>    
                    <!--<Label text="{i18n>processar_txt}" />
                    <Switch state="{fechamentoIFRS17Model>Prg/Acao}" customTextOn="{i18n>sim_txt}" customTextOff="{i18n>nao_txt}" enabled="false">
                        <layoutData>
